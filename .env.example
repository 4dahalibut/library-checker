# Library card credentials (required for holds functionality)
LIBRARY_BARCODE=your_library_barcode
LIBRARY_PIN=your_library_pin
LIBRARY_ACCOUNT_ID=your_account_id

# Anthropic API key (required for culture tagging)
ANTHROPIC_API_KEY=sk-ant-...

# Server configuration (optional)
PORT=3456
NODE_ENV=production

# Database path (optional, defaults to data/library.db)
DATABASE_PATH=data/library.db
